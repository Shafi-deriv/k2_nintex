{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Client Documents API",
    "description": "K2-compatible Swagger for retrieving client documents via CGI endpoint."
  },
  "host": "dtnr83vvte.execute-api.us-east-1.amazonaws.com",
  "basePath": "/api_v1/d/backoffice",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/client_documents.cgi": {
      "get": {
        "summary": "Get Client Documents",
        "description": "Retrieves client document records based on login ID.",
        "operationId": "getClientDocuments",
        "parameters": [
          {
            "name": "loginid",
            "in": "query",
            "required": true,
            "type": "string"
          },
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "Bearer token to be passed at runtime"
          },
          {
            "name": "x-api-key",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "API key passed at runtime"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with document data",
            "schema": {
              "type": "object"
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "type": "object",
              "properties": {
                "error": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
